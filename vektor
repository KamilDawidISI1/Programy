#include <iostream>
#include<vector>

using namespace std;

int main()
{
    cout << "Zajecia nr 3\n\n\n";

    vector<int>tab;

    tab.push_back(10);

    cout << tab[0] << endl;

    tab[0] = 8;

    cout << tab[0] << endl;

    tab[0] -= 2; //tab[0] = tab[0] -2

    cout << tab[0] << endl;

    for (int i = 0; i < tab.size(); i++)
    {
        cout << tab[i] << endl;
    }

// praca własna bez mieżenia czasu
vector<int>tab1;
    tab1.push_back(9);
    cout <<"\n\n\n" << tab1[0]<<endl;
    tab1[0] = 7;
    cout<<tab1[0]<<endl;
    tab1[0] += 4;
    cout<<tab1[0]<<endl;
    for (int j=0; j<tab1.size(); j++){
        cout << tab1[j] << endl;
    }


//praca własna z mierzeniem czasu : 2:41

#include <iostream>
#include <vector> 

using namespace std;
int main()
{
    vector<int>tab;
    tab.push_back(10);
    cout<< tab[0] <<endl;
    tab[0] = 8;
    cout<< tab[0] <<endl;
    tab[0] -=2;
    cout<< tab[0] <<endl;
    for(int i=0; i<tab.size(); i++){
        cout<< tab[i] <<endl;
    }
    
}

}
