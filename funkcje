#include <iostream>
#include <vector>

using namespace std;


void wypisz(vector<int>tab) {
	for (int i = 0; i < tab.size(); i++)
	{
		cout << tab[i] << ", ";
	}
	cout << endl;
}



int main()
{
	cout << "Zajęcia 4" << endl;
	vector<int>tab = { 8, 5, 4 , 3 };

	wypisz(tab);


}



porównywanie liczb w wektorze
int wynik = 0;

	if (tab[0] > tab[1]) {
		wynik = tab[0];
	}
	else {
		wynik = tab[1];
	}

	for (int j = 0; j < tab.size(); j++)
	{
		if (tab[j] > wynik)
		{
			wynik = tab[j];
		}
	}

	cout << wynik;
	
	
funkcja z porównywania liczb
int max(vector<int>tab) {
	int wynik = 0;

	if (tab[0] > tab[1]) {
		wynik = tab[0];
	}
	else {
		wynik = tab[1];
	}

	for (int j = 2; j < tab.size(); j++)
	{
		if (tab[j] > wynik)
		{
			wynik = tab[j];
		}
	}

	return wynik;
}

funkcja do szukania ulepszona 
int max(vector<int>tab) {
	int wynik = tab[0];

	for (int j = 1; j < tab.size(); j++)
	{
		if (tab[j] > wynik)
		{
			wynik = tab[j];
		}
	}

	return wynik;
}

-----------------------------------------------------------------

cały program
#include <iostream>
#include <vector>

using namespace std;


void wypisz(vector<int>tab) {
	for (int i = 0; i < tab.size(); i++)
	{
		cout << tab[i] << ", ";
	}
	cout << endl;
}

int max(vector<int>tab) {
	int wynik = tab[0];

	for (int j = 1; j < tab.size(); j++)
	{
		if (tab[j] > wynik)
		{
			wynik = tab[j];
		}
	}

	return wynik;
}


int main()
{
	vector<int>tab = { 8, 5, 4 , 3 };

	wypisz(tab);
    cout << max(tab);

}
