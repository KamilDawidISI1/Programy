#include <iostream>
#include <vector>

using namespace std;

void wypisz(vector<int>tab){
    for(int i=0; i<tab.size(); i++){
        cout << tab[i] << ", ";
    }
    cout << endl;
}

int max(vector<int>tab){
    int wynik = tab[0];
    for(int i=1; i<tab.size(); i++){
        if(tab[i]>wynik){
            wynik = tab[i];
        }
    }
    return wynik;
}

bool czyNalezy(vector<int>tab , int element){
    for(int i =0; i<tab.size(); i++){
        if(tab[i] == element){
            return true;
        }
    }
    return false;
}

bool czyZawiera(vector<int>tab1, vector<int>tab2){
    for(int i=0; i<tab2.size(); i++){
        if(!czyNalezy(tab1, tab2[i])){
            return false;
        }
    }
    return true;
}

int main()
{
    
    vector<int>tab1 = {1,2,3,4,5,6};
    vector<int>tab2 = {1,2,3};
    
    int element = 4;
    
    wypisz(tab1);
    wypisz(tab2);
    
    cout << max(tab1) << endl;
    cout << max(tab2) << endl;
    
    cout << czyNalezy(tab1, element) << endl;
    cout << czyNalezy(tab2, element) << endl;
    
    cout << czyZawiera(tab1, tab2);
    
    return 0;
}

